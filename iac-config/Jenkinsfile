pipeline {
    agent any
    
    tools {
      terraform 'Terraform'
      }

    stages {
        stage ('Clean Workspace') {
            steps {
              cleanWs()
              }
           }

        stage ('Checkout-scm') {
            steps {
                script {
                    git branch: 'development', url: 'https://github.com/irfan-1117/vpc-projects.git'
                }
            }
        }
           
        stage('aws-cli') {
            steps {
               sh  'aws s3 ls'
            }
        }
    }
}
